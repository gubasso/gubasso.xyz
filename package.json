{
  "name": "gubasso.xyz",
  "version": "0.1.0",
  "private": true,
  "repository": "gubasso/gubasso.xyz",
  "scripts": {
    "dev": "vite dev",
    "watch:pdf": "watchexec -w src -f '*' -w . -f 'generatePdf*' -- node generatePdf.js",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --list-different --ignore-unknown --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.1.0",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.3.5",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "glob": "^11.0.0",
    "mdsvex": "^0.11.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "vite": "^5.0.11",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "cal-heatmap": "^4.2.3",
    "iconify-icon": "^1.0.8",
    "js-yaml": "^4.1.0",
    "path-parse": "^1.0.7",
    "puppeteer": "^22.12.1",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-footnotes": "2.0",
    "remark-github": "^12.0.0",
    "remark-mermaidjs": "^6.0.0",
    "remark-toc": "^9.0.0",
    "remark-unwrap-images": "^4.0.0",
    "svelte-preprocess": "^5.1.3"
  }
}
