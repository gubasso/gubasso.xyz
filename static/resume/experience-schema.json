{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Employment History",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "company": {
        "type": "string"
      },
      "date-of-employment": {
        "type": "string"
      },
      "roles": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "projects": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": ["title", "company", "date-of-employment", "roles", "projects"]
  }
}
