{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Teachings and Lectures",
  "type": "object",
  "patternProperties": {
    "^[a-z0-9]+(-[a-z0-9]+)*$": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      },
      "required": ["title"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
